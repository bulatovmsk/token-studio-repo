{
  "cover": {
    "numbers": {
      "main": {
        "value": "3",
        "type": "number"
      },
      "vertical": {
        "value": "3",
        "type": "number"
      },
      "large": {
        "value": "1.5",
        "type": "number"
      },
      "square-small-story": {
        "value": 6,
        "type": "number"
      },
      "square": {
        "value": "3",
        "type": "number"
      },
      "marketing": {
        "value": "1.5",
        "type": "number"
      },
      "featured": {
        "value": "1.25",
        "type": "number"
      },
      "medium": {
        "value": "2",
        "type": "number"
      },
      "announces": {
        "value": "2",
        "type": "number"
      }
    },
    "width": {
      "main": {
        "value": "({container.width} - ( {cover.gaps-count.main} * {spacing.150})) / {cover.numbers.main}",
        "type": "sizing"
      },
      "announces": {
        "value": "({container.width} - ( {cover.gaps-count.announces} * {spacing.150})) / {cover.numbers.announces}",
        "type": "sizing"
      },
      "vertical": {
        "value": "({container.width} - ( {cover.gaps-count.vertical}  *{spacing.150} )) / {cover.numbers.vertical}",
        "type": "sizing"
      },
      "large": {
        "value": "({container.width} - ( {cover.gaps-count.large}  *{spacing.150} )) / {cover.numbers.large}",
        "type": "sizing"
      },
      "square-small-story": {
        "value": "({container.width} - ( {cover.gaps-count.square-small-story} * {spacing.150})) / {cover.numbers.square-small-story}",
        "type": "sizing"
      },
      "square": {
        "value": "({container.width} - ( {cover.gaps-count.square} *{spacing.150} )) / {cover.numbers.square}",
        "type": "sizing"
      },
      "marketing": {
        "value": "({container.width} - ( {cover.gaps-count.marketing}  *{spacing.150} )) / {cover.numbers.marketing}",
        "type": "sizing"
      },
      "medium": {
        "value": "({container.width} - ({cover.gaps-count.medium}  *{spacing.150} )) / {cover.numbers.medium}",
        "type": "sizing"
      },
      "featured": {
        "value": "({container.width} - ({cover.gaps-count.featured}  *{spacing.150} )) / {cover.numbers.featured}",
        "type": "sizing"
      }
    },
    "sizes": {
      "main": {
        "value": {
          "width": "{cover.width.main}",
          "height": "{cover.height.main}"
        },
        "type": "composition"
      },
      "announces": {
        "value": {
          "width": "{cover.width.announces}"
        },
        "type": "composition"
      },
      "vertical": {
        "value": {
          "width": "{cover.width.vertical}",
          "height": "{cover.height.vertical}"
        },
        "type": "composition"
      },
      "large": {
        "value": {
          "width": "{cover.width.large}",
          "height": "{cover.height.large}"
        },
        "type": "composition"
      },
      "square-small-story": {
        "value": {
          "width": "{cover.width.square-small-story}",
          "height": "{cover.height.square-small-story}"
        },
        "type": "composition"
      },
      "square": {
        "value": {
          "width": "{cover.width.square}",
          "height": "{cover.height.square}"
        },
        "type": "composition"
      },
      "marketing": {
        "value": {
          "width": "{cover.width.marketing}",
          "height": "{cover.height.marketing}"
        },
        "type": "composition"
      },
      "medium": {
        "value": {
          "width": "{cover.width.medium}",
          "height": "{cover.height.medium}"
        },
        "type": "composition"
      },
      "featured": {
        "value": {
          "width": "{cover.width.featured}",
          "height": "{cover.height.featured}"
        },
        "type": "composition"
      }
    },
    "height": {
      "main": {
        "value": "ceil({cover.width.main} / 16 * 9)",
        "type": "sizing"
      },
      "large": {
        "value": "ceil({cover.width.large} / 16 * 9)",
        "type": "sizing"
      },
      "featured": {
        "value": "ceil({cover.width.featured} / 16 * 9)",
        "type": "sizing"
      },
      "vertical": {
        "value": "ceil({cover.width.vertical} * 1.5)",
        "type": "sizing"
      },
      "square-small-story": {
        "value": "ceil({cover.width.square-small-story})",
        "type": "sizing"
      },
      "square": {
        "value": "ceil({cover.width.square})",
        "type": "sizing"
      },
      "marketing": {
        "value": "ceil({cover.width.marketing})",
        "type": "sizing"
      },
      "medium": {
        "value": "ceil({cover.width.medium} / 16 * 9)",
        "type": "sizing"
      }
    },
    "gaps-count": {
      "main": {
        "value": "if(ceil({cover.numbers.main}) - 1 < 1, 0, ceil({cover.numbers.main}) - 1)",
        "type": "sizing"
      },
      "announces": {
        "value": "if(ceil({cover.numbers.announces}) - 1 < 1, 0, ceil({cover.numbers.announces}) - 1)",
        "type": "sizing"
      },
      "vertical": {
        "value": "if(ceil({cover.numbers.vertical}) - 1 < 1, 0, ceil({cover.numbers.vertical}) - 1)",
        "type": "sizing"
      },
      "large": {
        "value": "if(ceil({cover.numbers.large}) - 1 < 1, 0, ceil({cover.numbers.large}) - 1)",
        "type": "sizing"
      },
      "square-small-story": {
        "value": "if(ceil({cover.numbers.square-small-story}) - 1 < 1, 0, ceil({cover.numbers.square-small-story}) - 1)",
        "type": "sizing"
      },
      "square": {
        "value": "if(ceil({cover.numbers.square}) - 1 < 1, 0, ceil({cover.numbers.square}) - 1)",
        "type": "sizing"
      },
      "marketing": {
        "value": "if(ceil({cover.numbers.marketing}) - 1 < 1, 0, ceil({cover.numbers.marketing}) - 1)",
        "type": "sizing"
      },
      "featured": {
        "value": "if(ceil({cover.numbers.featured}) - 1 < 1, 0, ceil({cover.numbers.featured}) - 1)",
        "type": "sizing"
      },
      "medium": {
        "value": "if(ceil({cover.numbers.medium}) - 1 < 1, 0, ceil({cover.numbers.medium}) - 1)",
        "type": "sizing"
      }
    }
  },
  "container": {
    "width": {
      "value": "{container.device-width} - {screen-padding.left} - {screen-padding.right}",
      "type": "sizing"
    },
    "device-width": {
      "value": "744",
      "type": "sizing"
    }
  },
  "screen-padding": {
    "left": {
      "value": "{screen-padding.ios.ipad}",
      "type": "sizing"
    },
    "right": {
      "value": "50",
      "type": "sizing"
    }
  }
}