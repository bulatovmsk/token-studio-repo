{
  "cover": {
    "numbers": {
      "main": {
        "value": "2.5",
        "type": "number"
      },
      "vertical": {
        "value": "4",
        "type": "number"
      },
      "large": {
        "value": "2",
        "type": "number"
      },
      "square-small-2-story": {
        "value": "6",
        "type": "number"
      },
      "square-small-1-story": {
        "value": "6",
        "type": "number"
      },
      "square": {
        "value": "5",
        "type": "number"
      },
      "marketing": {
        "value": "3",
        "type": "number"
      },
      "medium": {
        "value": "3",
        "type": "number"
      }
    },
    "width": {
      "main": {
        "value": "({container.width} - ( {cover.gaps-count.main} * {spacing.125})) / {cover.numbers.main}",
        "type": "sizing"
      },
      "vertical": {
        "value": "({container.width} - (( {cover.gaps-count.vertical} - 1)  *{spacing.125} )) / {cover.gaps-count.vertical}",
        "type": "sizing"
      },
      "large": {
        "value": "({container.width} - (( {cover.gaps-count.large} - 1)  *{spacing.125} )) / {cover.gaps-count.large}",
        "type": "sizing"
      },
      "square-small-2-story": {
        "value": "({container.width} - (( {cover.gaps-count.square-small-2-story} - 1)  *{spacing.125} )) / {cover.gaps-count.square-small-2-story}",
        "type": "sizing"
      },
      "square-small-1-story": {
        "value": "({container.width} - (( {cover.numbers.square-small-1-story} - 1)  *{spacing.125} )) / {cover.numbers.square-small-1-story}",
        "type": "sizing"
      },
      "square": {
        "value": "({container.width} - (( {cover.gaps-count.square} - 1)  *{spacing.125} )) / {cover.gaps-count.square}",
        "type": "sizing"
      },
      "marketing": {
        "value": "({container.width} - (( {cover.gaps-count.marketing} - 1)  *{spacing.125} )) / {cover.gaps-count.marketing}",
        "type": "sizing"
      },
      "medium": {
        "value": "({container.width} - (( {cover.gaps-count.medium} - 1)  *{spacing.125} )) / {cover.gaps-count.medium}",
        "type": "sizing"
      }
    },
    "sizes": {
      "main": {
        "value": {
          "width": "{cover.width.main}",
          "height": "{cover.height.main}"
        },
        "type": "composition"
      },
      "vertical": {
        "value": {
          "width": "{cover.width.vertical}",
          "height": "{cover.height.vertical}"
        },
        "type": "composition"
      },
      "large": {
        "value": {
          "width": "{cover.width.large}",
          "height": "{cover.height.large}"
        },
        "type": "composition"
      },
      "square-small-2-story": {
        "value": {
          "width": "{cover.width.square-small-2-story}",
          "height": "{cover.height.square-small-2-story}"
        },
        "type": "composition"
      },
      "square-small-1-story": {
        "value": {
          "width": "{cover.width.square-small-1-story}",
          "height": "{cover.height.square-small-1-story}"
        },
        "type": "composition"
      },
      "square": {
        "value": {
          "width": "{cover.width.square}",
          "height": "{cover.height.square}"
        },
        "type": "composition"
      },
      "marketing": {
        "value": {
          "width": "{cover.width.marketing}",
          "height": "{cover.height.marketing}"
        },
        "type": "composition"
      },
      "medium": {
        "value": {
          "width": "{cover.width.medium}",
          "height": "{cover.height.medium}"
        },
        "type": "composition"
      }
    },
    "height": {
      "main": {
        "value": "{cover.width.main} / 16 * 9",
        "type": "sizing"
      },
      "large": {
        "value": "{cover.width.large} / 16 * 9",
        "type": "sizing"
      },
      "vertical": {
        "value": "{cover.width.vertical} * 1.5",
        "type": "sizing"
      },
      "square-small-2-story": {
        "value": "{cover.width.square-small-2-story}",
        "type": "sizing"
      },
      "square-small-1-story": {
        "value": "{cover.width.square-small-1-story}",
        "type": "sizing"
      },
      "square": {
        "value": "{cover.width.square}",
        "type": "sizing"
      },
      "marketing": {
        "value": "{cover.width.marketing}",
        "type": "sizing"
      },
      "medium": {
        "value": "{cover.width.medium} / 16 * 9",
        "type": "sizing"
      }
    },
    "gaps-count": {
      "main": {
        "value": "if(({cover.numbers.main}-({cover.numbers.main}%1)-1)<1,1,({cover.numbers.main}-({cover.numbers.main}%1)-1))",
        "type": "sizing"
      },
      "vertical": {
        "value": "if(({cover.numbers.vertical}-({cover.numbers.vertical}%1)-1)<1,1,({cover.numbers.vertical}-({cover.numbers.vertical}%1)-1))",
        "type": "sizing"
      },
      "large": {
        "value": "if(({cover.numbers.large}-({cover.numbers.large}%1)-1)<1,1,({cover.numbers.large}-({cover.numbers.large}%1)-1))",
        "type": "sizing"
      },
      "square-small-2-story": {
        "value": "if(({cover.numbers.square-small-2-story}-({cover.numbers.square-small-2-story}%1)-1)<1,1,({cover.numbers.square-small-2-story}-({cover.numbers.square-small-2-story}%1)-1))",
        "type": "sizing"
      },
      "square-small-1-story": {
        "value": "if(({cover.numbers.square-small-1-story}-({cover.numbers.square-small-1-story}%1)-1)<1,1,({cover.numbers.square-small-1-story}-({cover.numbers.square-small-1-story}%1)-1))",
        "type": "sizing"
      },
      "square": {
        "value": "if(({cover.numbers.square}-({cover.numbers.square}%1)-1)<1,1,({cover.numbers.square}-({cover.numbers.square}%1)-1))",
        "type": "sizing"
      },
      "marketing": {
        "value": "if(({cover.numbers.marketing}-({cover.numbers.marketing}%1)-1)<1,1,({cover.numbers.marketing}-({cover.numbers.marketing}%1)-1))",
        "type": "sizing"
      },
      "medium": {
        "value": "if(({cover.numbers.medium}-({cover.numbers.medium}%1)-1)<1,1,({cover.numbers.medium}-({cover.numbers.medium}%1)-1))",
        "type": "sizing"
      }
    }
  },
  "container": {
    "width": {
      "value": "{container.device-width} - {screen-padding.left} - {screen-padding.right}",
      "type": "sizing"
    },
    "device-width": {
      "value": "375px",
      "type": "sizing"
    }
  },
  "screen-padding": {
    "left": {
      "value": "{screen-padding.mobile}",
      "type": "sizing"
    },
    "right": {
      "value": "36px",
      "type": "sizing"
    }
  }
}