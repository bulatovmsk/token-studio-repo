{
  "cover": {
    "numbers": {
      "main": {
        "value": "1.5",
        "type": "number"
      },
      "vertical": {
        "value": "2.5",
        "type": "number"
      },
      "large": {
        "value": "1",
        "type": "number"
      },
      "square-small-story": {
        "value": "3",
        "type": "number"
      },
      "square": {
        "value": "2",
        "type": "number"
      },
      "marketing": {
        "value": "1",
        "type": "number"
      },
      "medium": {
        "value": "1.25",
        "type": "number"
      }
    },
    "width": {
      "main": {
        "value": "({container.width} - ( {cover.gaps-count.main} *6)) / {cover.numbers.main}",
        "type": "sizing"
      },
      "vertical": {
        "value": "({container.width} - ( {cover.gaps-count.vertical}  *{spacing.125} )) / {cover.numbers.vertical}",
        "type": "sizing"
      },
      "large": {
        "value": "({container.width} - ( {cover.gaps-count.large}  *{spacing.125} )) / {cover.numbers.large}",
        "type": "sizing"
      },
      "square-small-story": {
        "value": "({container.width} - ( {cover.numbers.square-small-story}  *{spacing.125} )) / {cover.numbers.square-small-story}",
        "type": "sizing"
      },
      "square": {
        "value": "({container.width} - ( {cover.gaps-count.square} *{spacing.125} )) / {cover.numbers.square}",
        "type": "sizing"
      },
      "marketing": {
        "value": "({container.width} - ( {cover.gaps-count.marketing}  *{spacing.125} )) / {cover.numbers.marketing}",
        "type": "sizing"
      },
      "medium": {
        "value": "({container.width} - (( {cover.gaps-count.medium} - 1)  *{spacing.125} )) / {cover.numbers.medium}",
        "type": "sizing"
      }
    },
    "sizes": {
      "main": {
        "value": {
          "width": "{cover.width.main}",
          "height": "{cover.height.main}"
        },
        "type": "composition"
      },
      "vertical": {
        "value": {
          "width": "{cover.width.vertical}",
          "height": "{cover.height.vertical}"
        },
        "type": "composition"
      },
      "large": {
        "value": {
          "width": "{cover.width.large}",
          "height": "{cover.height.large}"
        },
        "type": "composition"
      },
      "square-small-story": {
        "value": {
          "width": "{cover.width.square-small-story}",
          "height": "{cover.height.square-small-story}"
        },
        "type": "composition"
      },
      "square": {
        "value": {
          "width": "{cover.width.square}",
          "height": "{cover.height.square}"
        },
        "type": "composition"
      },
      "marketing": {
        "value": {
          "width": "{cover.width.marketing}",
          "height": "{cover.height.marketing}"
        },
        "type": "composition"
      },
      "medium": {
        "value": {
          "width": "{cover.width.medium}",
          "height": "{cover.height.medium}"
        },
        "type": "composition"
      }
    },
    "height": {
      "main": {
        "value": "{cover.width.main} / 16 * 9",
        "type": "sizing"
      },
      "large": {
        "value": "{cover.width.large} / 16 * 9",
        "type": "sizing"
      },
      "vertical": {
        "value": "{cover.width.vertical} * 1.5",
        "type": "sizing"
      },
      "square-small-story": {
        "value": "{cover.width.square-small-story}",
        "type": "sizing"
      },
      "square": {
        "value": "{cover.width.square}",
        "type": "sizing"
      },
      "marketing": {
        "value": "{cover.width.marketing}",
        "type": "sizing"
      },
      "medium": {
        "value": "{cover.width.medium} / 16 * 9",
        "type": "sizing"
      }
    },
    "gaps-count": {
      "main": {
        "value": "if(floor({cover.numbers.main} + 0.5) - 1 < 1, 0, floor({cover.numbers.main} + 0.5) - 1)",
        "type": "sizing"
      },
      "vertical": {
        "value": "if(floor({cover.numbers.vertical} + 0.5) - 1 < 1, 0, floor({cover.numbers.vertical} + 0.5) - 1)",
        "type": "sizing"
      },
      "large": {
        "value": "if(floor({cover.numbers.large} + 0.5) - 1 < 1, 0, floor({cover.numbers.large} + 0.5) - 1)",
        "type": "sizing"
      },
      "square-small-story": {
        "value": "if(floor({cover.numbers.square-small-story} + 0.5) - 1 < 1, 0, floor({cover.numbers.square-small-story} + 0.5) - 1)",
        "type": "sizing"
      },
      "square": {
        "value": "if(floor({cover.numbers.square} + 0.5) - 1 < 1, 0, floor({cover.numbers.square} + 0.5) - 1)",
        "type": "sizing"
      },
      "marketing": {
        "value": "if(floor({cover.numbers.marketing} + 0.5) - 1 < 1, 0, floor({cover.numbers.marketing} + 0.5) - 1)",
        "type": "sizing"
      },
      "medium": {
        "value": "if(floor({cover.numbers.medium} + 0.5) - 1 < 1, 0, floor({cover.numbers.medium} + 0.5) - 1)",
        "type": "sizing"
      }
    }
  },
  "container": {
    "width": {
      "value": "{container.device-width} - {screen-padding.left} - {screen-padding.right}",
      "type": "sizing"
    },
    "device-width": {
      "value": "393",
      "type": "sizing"
    }
  },
  "screen-padding": {
    "left": {
      "value": "12",
      "type": "sizing"
    },
    "right": {
      "value": "36",
      "type": "sizing"
    }
  }
}