      "rich": {
        "value": "({container.width} - (( {cover.numbers.rich} - 1)  *{spacing.200} )) / {cover.numbers.rich}",
        "type": "sizing"
      }
    },
    "sizes": {
      "vertical": {
        "value": {
          "width": "{cover.width.vertical}",
          "height": "{cover.width.vertical} / 2 *3"
        },
        "type": "composition"
      },
      "main": {
        "value": {
          "width": "{cover.width.main}",
          "height": "{cover.width.main} / 16 * 9"
        },
        "type": "composition"
      },
      "rich": {
        "value": {
          "width": "{cover.width.rich}",
          "height": "{cover.width.rich} / 10 * 12"
        },
        "type": "composition"
      },
      "rich1x1": {
        "value": {
          "width": "{cover.width.rich}",
          "height": "{cover.width.rich} / 10 * 12"
        },
        "type": "composition"
      },
      "rich2x1": {
        "value": {
          "width": "{cover.width.rich}",
          "height": "(({cover.width.rich} / 10 * 12) - {spacing.300}) /2"
